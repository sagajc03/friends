<div id="<%= dom_id friend %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <h2 class="text-2xl font-semibold text-black">
      <%= friend.first_name %> 
      <%= friend.last_name %>
    </h2>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= friend.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Phone:</strong>
    <%= friend.phone %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Twitter:</strong>
    <%= friend.twitter %>
  </div>
</div>
